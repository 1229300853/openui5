<core:View controllerName="sap.m.sample.SlideTile.Page"
	xmlns="sap.m" xmlns:core="sap.ui.core">
	<SlideTile class="sapUiTinyMargin">
		<tiles>
			<GenericTile
				backgroundImage="test-resources/sap/m/demokit/sample/SlideTile/images/NewsImage1.png"
				frameType="TwoByOne" press="press">
				<tileContent>
					<TileContent footer="August 21, 2013">
						<content>
							<NewsContent
								contentText="SAP Unveils Powerful New Player Comparison Tool Exclusively on NFL.com"
								subheader="SAP News">
							</NewsContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
			<GenericTile
				backgroundImage="test-resources/sap/m/demokit/sample/SlideTile/images/NewsImage3.png"
				state="Failed" frameType="TwoByOne">
				<tileContent>
					<TileContent footer="August 21, 2013">
						<content>
							<NewsContent
								contentText="SAP Unveils Powerful New Player Comparison Tool Exclusively on NFL.com"
								subheader="SAP News">
							</NewsContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		</tiles>
	</SlideTile>
</core:View>