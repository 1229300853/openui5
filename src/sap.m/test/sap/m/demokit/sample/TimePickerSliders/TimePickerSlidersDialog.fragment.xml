<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		title="TimePickerSliders"
		class="sapUiPopupWithPadding"
	>
		<Label text="hh:mm:ss a"/>
		<TimePickerSliders
			id="TPS2"
			valueFormat="hh:mm:ss a"
			displayFormat="hh:mm:ss a"
			change="handleChange"
			height="400px"
		/>
		<beginButton>
			<Button
				text="OK"
				press="handleOKPress"
			></Button>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>