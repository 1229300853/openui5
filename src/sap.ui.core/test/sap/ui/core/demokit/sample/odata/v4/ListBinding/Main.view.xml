<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	>
	<VBox>
		<Table items="{/EMPLOYEES}">
			<columns>
				<Column>
					<Text text="Name"/>
				</Column>
				<Column>
					<Text text="Age"/>
				</Column>
				<Column>
					<Text text="City"/>
				</Column>
				<Column>
					<Text text="Entry Date"/>
				</Column>
				<Column>
					<Text text="Entry Date 2"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Name}"/>
					</cells>
					<cells>
						<Text text="{AGE}"/>
					</cells>
					<cells>
						<Text text="{LOCATION/City/CITYNAME}"/>
					</cells>
					<cells>
						<!-- type is automatically determined by binding -->
						<Text text="{ENTRYDATE}"/>
					</cells>
					<cells>
						<!-- app specified type wins over automatically determined type -->
						<Text text="{path: 'ENTRYDATE', type: 'sap.ui.model.type.String'}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</mvc:View>
