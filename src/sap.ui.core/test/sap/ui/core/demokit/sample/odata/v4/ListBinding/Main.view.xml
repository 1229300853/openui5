<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	>
	<VBox>
		<Table items="{/EMPLOYEES}">
			<columns>
				<Column>
					<Text text="Name"/>
				</Column>
				<Column>
					<Text text="Age"/>
				</Column>
				<Column>
					<Text text="City"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{Name}"/>
					</cells>
					<cells>
						<Text text="{AGE}"/>
					</cells>
					<cells>
						<Text text="{LOCATION/City/CITYNAME}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</mvc:View>
