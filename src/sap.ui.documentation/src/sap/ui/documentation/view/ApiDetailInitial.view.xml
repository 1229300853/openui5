<mvc:View
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:l="sap.ui.layout">

	<Page showHeader="false">

		<content>
			<l:BlockLayout
					background="Light">
				<l:BlockLayoutRow>
					<l:BlockLayoutCell
							class="sapUiNoContentPadding">
						<Image
								src="./resources/sap/ui/documentation/images/M_and_L_cropped_277889.png"
								width="100%"/>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>

				<l:BlockLayoutRow>
					<l:BlockLayoutCell
							title="UI5 Library Namespaces">
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>

				<l:BlockLayoutRow>
					<l:BlockLayoutCell
							width="2">
						<l:VerticalLayout
								width="100%">
							<Title
									text="The SAPUI5 Core Runtime"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="Contains the UI5 jQuery plugins (jQuery.sap.*), the Core and all it's components, base classes for Controls, Components and the Model View Controller classes."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.ui.core namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.core.Control"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.core.Core"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Fiori library"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="SAPUI5 library with controls specialized for Fiori applications."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.f namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.f.DynamicPage"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.f.FlexibleColumnLayout"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Fiori for Tools"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="SAPUI5 library with controls specialized for administrative applications."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.tnt namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.tnt.ToolPage"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.tnt.ToolHeader"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>

				<l:BlockLayoutRow>
					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Layout Controls"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="SAPUI5 library specialized for layout controls"
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.ui.layout namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.layout.form"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.layout.BlockLayout"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Object Pages"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="SAPUI5 library with controls specialized for Fiori applications."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.uxap namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.uxap.ObjectPageLayout"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.uxap.ObjectPageHeaderContent"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell
							width="2">
						<l:VerticalLayout
								width="100%">
							<Title
									text="Main Controls"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="The main UI5 control library, with responsive controls that can be used in touch devices as well as desktop browsers."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.m namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.m.Page"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.m.NavContainer"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>

				<l:BlockLayoutRow>
					<l:BlockLayoutCell
							width="2">
						<l:VerticalLayout
								width="100%">
							<Title
									text="Smart Controls"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="The sap.ui namespace is the central OpenAjax compliant entry point for UI related JavaScript functionality provided by SAP."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.ui.comp namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.comp.smartform"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.comp.smartfilterbar"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Charts"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="Chart controls based on the SAP BI CVOM charting library"
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.ui.comp namespace"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="Link to API reference page"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="Link to API reference page"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>

					<l:BlockLayoutCell>
						<l:VerticalLayout
								width="100%">
							<Title
									text="Tables"
									titleStyle="H2"
									class="sapUiSmallMarginBottom"/>
							<Text
									text="Table-like controls support touch and non touch desktop and tablet scenarios."
									class="sapUiSmallMarginBottom"/>
							<Link
									text="sap.ui.table"
									class="sapUiMediumMarginBottom"/>
							<Title
									text="Further Links"
									titleStyle="H5"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sao.ui.table.Table"
									class="sapUiTinyMarginBottom"/>
							<Link
									text="sap.ui.table.AnalyticalTable"/>
						</l:VerticalLayout>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>

				<l:BlockLayoutRow>
					<l:BlockLayoutCell
							class="colorPatternFContrast">
						<Image
								src="./resources/sap/ui/documentation/images/SAP_grad_R_min.png"
								height="2.5rem">
						</Image>
					</l:BlockLayoutCell>
					<l:BlockLayoutCell
							class="colorPatternFContrast">
						<HBox
								width="100%"
								justifyContent="End">
							<Link
									class="brightLink sapUiLargeMarginEnd"
									text="Legal Disclosure"/>
							<Link
									class="brightLink sapUiLargeMarginEnd"
									text="Privacy"/>
							<Link
									class="brightLink"
									text="Terms of use"/>
						</HBox>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>
			</l:BlockLayout>
		</content>
	</Page>
</mvc:View>